{
    "builds": [{
        "src": "django-polls-application/wsgi.py",
        "use": "@vercel/python",
        "connfig": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "django-polls-application/wsgi.py"
        }
    ]
}